{
  "name": "fashionista",
  "version": "1.0.0",
  "description": "fashion vlogging platform",
  "main": "index.ts",
  "scripts": {
    "start": "node src/index.ts",
    "server": "nodemon src/index.ts",
    "db:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n vlog",
    "db:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n vlog",
    "db:migrate": "ts-node ./node_modules/typeorm/cli.js migration:run"
  },
  "keywords": [
    "fashion",
    "vlog",
    "vlogging",
    "nodejs",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/argon2": "^0.15.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.7",
    "@types/uuid": "^8.3.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "apollo-server": "^2.19.0",
    "apollo-server-express": "^2.19.0",
    "argon2": "^0.27.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.1"
  }
}
